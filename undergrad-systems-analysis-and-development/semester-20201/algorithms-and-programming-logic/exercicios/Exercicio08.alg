algoritmo "Exercicio08"

var

   depositoPoupanca, rendPoupanca : real

inicio

   escreva("Digite o valor do deposito: ")
   leia(depositoPoupanca)

   rendPoupanca <- depositoPoupanca + (depositoPoupanca * 0.013)

   escreva("O valor do deposito apos 1 mes de aplicacao e: ", rendPoupanca)

fimalgoritmo