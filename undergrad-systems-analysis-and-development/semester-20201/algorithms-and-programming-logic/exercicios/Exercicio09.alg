algoritmo "Exercicio09"

var

   num1, num2, somaQuadrados : inteiro
   
inicio

   escreva("Digite o primeiro valor: ")
   leia(num1)
   escreva("Digite o segundo valor: ")
   leia(num2)

   somaQuadrados <- (num1 * num1) + (num2 * num2)
   
   escreva("A soma dos quadrados e: ", somaQuadrados)

fimalgoritmo